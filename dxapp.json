  "summary": "Uploads genomic data files to the Illumina Connected Insights (ICI) platform",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "billTo": "org-emee_1",
  "properties": {
      "githubRelease": ""
  },
  "developers": [
      "user-spaul4",
      "org-emee_1"
  ],
  "authorizedUsers": [
      "org-emee_1"
  ],
  "inputSpec": [
      {
          "name": "run_folder",
          "label": "Run Folder",
          "class": "string",
          "optional": false,
          "help": "Path to folder containing pipeline output. The directory structure *must* match the directory tree specification outlined in the workflow definition - log into ICI and consult the manual for further details."
